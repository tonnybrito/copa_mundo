<%= form_for @team, html: { multipart: true } do |form| %>
  <% if team.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(team.errors.count, "error") %> prohibited this team from being saved:</h2>

      <ul>
      <% team.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :nome %>
    <%= form.text_field :nome, id: :team_nome %>
  </div>

  <div class="field">
    <%= form.label :flag %>
    <%= form.file_field :flag %>
  </div>

  <div class="field">
    <%= form.label :team_id, "teste" %>
    <%= form.collection_select(:continent_id, Continent.all, :id, :nome, {:include_blank => true}) %>
  </div>


  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
